{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).jpg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        }
      ]
    },
    {
      "source": "/(.*).JPG",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        }
      ]
    },
    {
      "source": "/(.*).mp4",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    },
    {
      "source": "/(.*).MP4",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    }
  ]
}

